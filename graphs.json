    {
        "id": "v1w2x3y4.z5a6b7",
        "type": "function",
        "z": "2d60f9e1.9f68e6",
        "name": "Processar Distância (<100cm Alerta)",
        "func": "// Payload é uma string, ex: \"150.75\"\nvar dist_cm = parseFloat(msg.payload);\n\nif (isNaN(dist_cm)) {\n    return null; // Ignora se não for um número\n}\n\nvar status_texto = \"\";\n\n// Lógica do Alerta: < 100cm = Alerta de proximidade\nif (dist_cm < 100) {\n    status_texto = \"ALERTA: Próximo!\";\n} else {\n    status_texto = \"OK - Distância Segura\";\n}\n\n// Saída 1: Valor numérico para Gauge e Gráfico\nvar msgDist = {\n    payload: dist_cm,\n    topic: \"Distância\" // Para o gráfico\n};\n\n// Saída 2: Texto de status para o Alerta\nvar msgStatus = {\n    payload: status_texto\n};\n\nreturn [ msgDist, msgStatus ];",
        "outputs": 2,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
