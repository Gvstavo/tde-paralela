        "id": "c9c72e27.9b17",
        "type": "function",
        "z": "2d60f9e1.9f68e6",
        "name": "Dividir Temp/Umidade",
        "func": "// O nó MQTT já converteu o JSON em um objeto: msg.payload\n// msg.payload = {temperatura: 25.5, umidade: 60.1}\n\n// 1. Mensagem para Temperatura (saída 1)\nvar msgTemp = {\n    payload: msg.payload.temperatura,\n    topic: \"Temperatura\"\n};\n\n// 2. Mensagem para Umidade (saída 2)\nvar msgHumid = {\n    payload: msg.payload.umidade,\n    topic: \"Umidade\"\n};\n\n// Retorna as duas mensagens em saídas separadas\nreturn [ msgTemp, msgHumid ];",
